{
  "name": "Auto WSM",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "I don't want to answer \"Do you know 7 core values?\"",
  "homepage_url": "https://github.com/baclv",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://wsm.sun-asterisk.vn/*",
        "https://wsm.framgia.vn/*"
      ],
      "js": [
        "js/script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "js/inject.js",
    "css/main.css"
  ]
}
